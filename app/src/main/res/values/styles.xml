<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="textStyle">
        <item name="android:layout_marginLeft">@dimen/_5sdp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/_15sdp</item>
    </style>
    <style name="FrgamentHeadingTextStyle">

        <item name="android:fontFamily">sans-serif-condensed-light</item>
        <item name="android:layout_marginLeft">@dimen/_5sdp</item>
        <item name="android:layout_marginTop">@dimen/_5sdp</item>
        <item name="android:textSize">@dimen/_23sdp</item>
    </style>
    <style name="RecyclerviewStyle">
        <item name="android:layout_marginLeft">@dimen/_5sdp</item>
        <item name="android:layout_marginTop">@dimen/_5sdp</item>
        <item name="android:layout_marginRight">@dimen/_5sdp</item>
    </style>


</resources>
